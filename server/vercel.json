{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "server/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "client/$1"
    }
  ],
  "builds": [
    {
      "src": "server/api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    }
  ]
}